<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/inatboxtvapkindir7_bg"
    android:weightSum="3">

    <FrameLayout
        android:id="@+id/fl_adplaceholder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/inatboxtvapkindir7_bannerrr"
        android:orientation="vertical"
        android:visibility="visible">

    </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_weight="3"
            android:gravity="center_horizontal">



            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/start"
                android:gravity="center"
                android:textStyle="bold"
                android:background="@drawable/inatboxtvapkindir7_group"
                android:layout_margin="16dp"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:text="Inat Box TV"
                android:textSize="17dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"/>
        </LinearLayout>

    <FrameLayout
        android:id="@+id/fl_b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/inatboxtvapkindir7_qb2"
        android:gravity="bottom"
        android:visibility="visible">
    </FrameLayout>






</LinearLayout>