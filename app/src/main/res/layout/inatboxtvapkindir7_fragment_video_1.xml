<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    android:background="@drawable/inatboxtvapkindir7_bg"
    tools:context="com.inatboxtvapkindirinfo.inat.fragments.inatboxtvapkindir7_Video_1">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"

            >



            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/play1"
                android:background="@drawable/inatboxtvapkindir7_group"
                android:gravity="center"
                android:textStyle="bold"
                android:layout_margin="17dp"
                android:layout_height="wrap_content"
                android:textColor="@color/Button_Text_Color"
                android:text="Playlist Nature"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/play2"
                android:background="@drawable/inatboxtvapkindir7_group"
                android:textStyle="bold"
                android:layout_margin="17dp"
                android:layout_height="wrap_content"
                android:textColor="@color/Button_Text_Color"
                android:text="Playlist footbal"
                android:gravity="center"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/play3"
                android:gravity="center"
                android:background="@drawable/inatboxtvapkindir7_group"
                android:textStyle="bold"
                android:layout_margin="17dp"
                android:layout_height="wrap_content"
                android:textColor="@color/Button_Text_Color"
                android:text="Playlist cars"

                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
    </ScrollView>



</LinearLayout>